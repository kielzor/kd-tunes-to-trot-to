{
  "name": "kd-tunes-to-trot-to",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint": "^5.5.0",
    "mocha": "^5.2.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-oauth-flow": "^1.2.0",
    "react-responsive-music-player": "^0.3.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "yarn": "^1.9.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "react-scripts start npm-run-all -p watch-css start-js",
    "build": "react-scripts build npm-run-all build-css build-js",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "jest": "^23.5.0"
  }
}
